{
  "bitwise"   : true,
  "camelcase" : true,
  "curly"     : true,
  "eqeqeq"    : true,
  "esversion" : 6,
  "forin"     : true,
  "freeze"    : true,
  "futurehostile": true,
  "immed"     : true,
  "indent"    : 2,
  "latedef"   : true,
  "newcap"    : true,
  "noarg"     : true,
  "nocomma"   : true,
  "noempty"   : true,
  "nonbsp"    : true,
  "nonew"     : true,
  "notypeof"  : true,
  "quotmark"  : "single",
  "shadow"    : "outer",
  "strict"    : true,
  "undef"     : true,
  "unused"    : true,
  "varstmt"   : true,

  "node": true,

  "globals": {
    "after"      : true,
    "afterEach"  : true,
    "before"     : true,
    "beforeEach" : true,
    "describe"   : true,
    "it"         : true
  }
}